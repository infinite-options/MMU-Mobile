{
  "expo": {
    "name": "MMU App",
    "slug": "meetmeup",
    "version": "1.1.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.infiniteoptions.meetmeupapp",
      "buildNumber": "13",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["$(PRODUCT_BUNDLE_IDENTIFIER)"],
            "CFBundleURLName": "google"
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.infiniteoptions.meetmeupapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assetBundlePatterns": [
      "assets/fonts/*"
    ],
    "owner": "pmarathay",
    "extra": {
      "eas": {
        "projectId": "a57a9873-7c6d-45b1-af9e-2ba19255cdb6"
      }
    },
    "scheme": "meetmeupapp",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.336598290180-e89eo7pqbfs7ilj3hjpj1fvr661cdnn2"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  },
  "web": {
    "favicon": "./assets/favicon.png"
  }
}
