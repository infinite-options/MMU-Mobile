      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBHZlcN8UeLNySmAf66WOfu1N_hDBaq0-I"
        }
      },
      "versionCode": 4
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assetBundlePatterns": [
      "assets/fonts/*"
    ],
    "owner": "pmarathay",
    "extra": {
      "eas": {
        "projectId": "a57a9873-7c6d-45b1-af9e-2ba19255cdb6"
      }
    },
    "scheme": "com.googleusercontent.apps.466541803518-41pdpb9pei96hihdt70db2e6a5hg8sj6",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
