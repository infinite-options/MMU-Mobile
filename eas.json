{
  "cli": {
    "version": ">= 12.1.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "env": {
          "GOOGLE_URL_SCHEME": "${process.env.EXPO_PUBLIC_GOOGLE_URL_SCHEME}"
        }
      },
      "android": {
        "env": {
          "GOOGLE_URL_SCHEME": "${process.env.EXPO_PUBLIC_GOOGLE_URL_SCHEME}"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "env": {
          "GOOGLE_URL_SCHEME": "${process.env.EXPO_PUBLIC_GOOGLE_URL_SCHEME}"
        }
      },
      "android": {
        "env": {
          "GOOGLE_URL_SCHEME": "${process.env.EXPO_PUBLIC_GOOGLE_URL_SCHEME}"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m-medium",
        "env": {
          "GOOGLE_URL_SCHEME": "${process.env.EXPO_PUBLIC_GOOGLE_URL_SCHEME}"
        }
      },
      "android": {
        "env": {
          "GOOGLE_URL_SCHEME": "${process.env.EXPO_PUBLIC_GOOGLE_URL_SCHEME}"
        }
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
